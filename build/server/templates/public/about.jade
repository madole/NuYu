extends layout
mixin circleHead(imageSrc, alt)
    .head__container
        .head__holder
            .head__circle
                img(src="#{imageSrc}", alt="#{alt}")

block content
    h1.about__header About our nurses
    .column__left
        h2.about__names Kate McDowell
        +circleHead("/assets/public/images/kate.jpg", "Kate")
        span #{pageData}
    .column__right
        h2.about__names Kate McDowell
        +circleHead("/assets/public/images/kate.jpg", "Kate")
        span #{pageData}
